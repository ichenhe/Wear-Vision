<resources>

    <!-- Style for app bar in window, like AlertDialog AppBar. -->
    <attr name="wv_windowAppBarStyle" format="reference" />
    <!-- Style for the content in app bar in window. -->
    <attr name="wv_windowAppBarContentStyle" format="reference" />
    <!-- Style for icon in window, like AlertDialog icon or icon in AppBar. -->
    <attr name="wv_windowIconStyle" format="reference" />

    <!-- Style for dialog content -->
    <attr name="wv_dialogContentStyle" format="reference" />

    <!-- Style for dialog message -->
    <attr name="wv_dialogMessageStyle" format="reference" />

    <!-- Style for the "positive" icon buttons -->
    <attr name="wv_positiveIconButtonStyle" format="reference" />

    <!-- Style for the "negative" icon buttons -->
    <attr name="wv_negativeIconButtonStyle" format="reference" />

    <!-- Style for the "neutral" icon buttons -->
    <attr name="wv_neutralIconButtonStyle" format="reference" />

    <!-- The "positive" icon button's drawable -->
    <attr name="wv_positiveButtonIcon" format="reference" />

    <!-- The "negative" icon button's drawable -->
    <attr name="wv_negativeButtonIcon" format="reference" />
</resources>